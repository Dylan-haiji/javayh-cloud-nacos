<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javayh.nacos.tc.dao.MenuMapper">

	<select id="findMenu" resultType="com.javayh.nacos.common.entity.SysMenu">
		SELECT
		*
		FROM
		sys_menu me
		where menu_name =#{menuName}
	</select>

</mapper>