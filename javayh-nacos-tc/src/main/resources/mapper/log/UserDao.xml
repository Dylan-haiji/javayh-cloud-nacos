<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javayh.nacos.tc.dao.UserMapper">

	<!-- 查询用户信息 -->
	<select id="selectUserByName" resultType="com.javayh.nacos.common.entity.SysUser" parameterType="String">
		select  *
		from sys_user
        where user_name= #{username}
        and del_flag = 0
	</select>

</mapper>